{
	"name": "chillerlan/phpunit-http",
	"description": "Add PSR-17 factories and a PSR-18 client to your tests",
	"license": "MIT",
	"type": "library",
	"keywords": ["phpunit", "http", "factories", "psr-17", "psr-18"],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		},
		{
			"name": "Contributors",
			"homepage":"https://github.com/chillerlan/phpunit-http/graphs/contributors"
		}
	],
	"homepage": "https://github.com/chillerlan/phpunit-http",
	"support": {
		"issues": "https://github.com/chillerlan/phpunit-http/issues",
		"source": "https://github.com/chillerlan/phpunit-http"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.5",
		"phan/phan": "^5.4",
		"chillerlan/php-httpinterface": "^6.0",
		"guzzlehttp/guzzle": "^7.8"
	},
	"suggest": {
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\PHPUnitHttp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\PHPUnitHttpTest\\": "tests/"
		}
	},
	"scripts": {
		"phpunit": "@php vendor/bin/phpunit",
		"phan": "@php vendor/bin/phan"
	},
	"config": {
		"lock": false,
		"sort-packages": true,
		"platform-check": true
	}
}
